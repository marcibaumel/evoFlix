<Window x:Class="WPF_API_DEMO.SumInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_API_DEMO"
        mc:Ignorable="d"
        Title="SumInfo" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="loadSunInfo" Margin="20">Load Sun Info</Button>
            <TextBlock x:Name="sunriseText" Margin="20"/>
            <TextBlock x:Name="sunsetText" Margin="20"/>
        </StackPanel>
    </Grid>
</Window>
