<Window x:Class="WPFAppCore.SunInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFAppCore"
        mc:Ignorable="d"
        Title="SunInfo" Height="450" Width="800" FontSize="22" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel>
            <Button x:Name="loadSunInfo" Margin="20" Click="loadSunInfo_Click">Load Sun Info</Button>
            <TextBlock x:Name="sunriseText" Margin="20"/>
            <TextBlock x:Name="sunsetText" Margin="20"/>
        </StackPanel>
    </Grid>
</Window>
