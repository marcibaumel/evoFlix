<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:evoFlix.WPF.Styles">


    <!--Colors-->


    <Color x:Key="ColorPrimary1">#799540</Color>
    <Color x:Key="ColorPrimary2">#50632b</Color>


    <!--Brushes -->
    <SolidColorBrush x:Key="BrushPrimaryKey1" Color="{StaticResource ColorPrimary1}"/>
    <SolidColorBrush x:Key="BrushPrimaryKey2" Color= "{StaticResource ColorPrimary2}"/>
    
    
</ResourceDictionary>